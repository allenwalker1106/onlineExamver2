<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <link rel="icon" href="https://p7.hiclipart.com/preview/692/12/989/letter-icon-eth-letter-d-png.jpg">
 
  <title>
    Diplo 
  </title>
  
  <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
  <!--     Fonts and icons     -->

  <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.css" rel="stylesheet">
  <!-- CSS Files -->
  <link href="assert/papper-dashboard/css/bootstrap.min.css" rel="stylesheet" />
  <link href="assert/papper-dashboard/css/paper-dashboard.css?v=2.0.0" rel="stylesheet" /> 
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>

<body class="">
  <div class="wrapper ">
    <div class="sidebar" data-color="white" data-active-color="danger">
      <div class="logo">
        <a href="/user_setting" class="simple-text logo-mini">
          <div class="logo-image-small" style="border-radius: 50%;">
            <img src=<%-user.profile_image_link%> >
          </div>
        </a>
        <a href="/user_setting" class="simple-text logo-normal">
          <%=user.name.firstname%>
        </a>
      </div>
      <div class="sidebar-wrapper">
        <ul class="nav">
          <li >
            <a href="/dashboard">
              <i class="fa fa-columns"></i>
              <p>Dashboard</p>
            </a>
          </li>
          <li class="active ">
            <a href="/questions">
              <i class="fa fa-list-ul"></i>
              <p>Questions</p>
            </a>
          </li>
          <li>
            <a href="./tests">
              <i class="fa fa-copy"></i>
              <p>Tests</p>
            </a>
          </li>
          <li>
            <a href="./links">
              <i class="fa fa-link"></i>
              <p>Links</p>
            </a>
          </li>
          <li>
            <a href="./groups">
                <i class="fa fa-users"></i>
                <p>Groups</p>
            </a>
          </li>
          <li>
            <a href="./result">
              <i class="fa fa-file"></i>
              <p>Results</p>
            </a>
          </li>
          <li>
            <a href="./files">
              <i class="fa fa-image"></i>
              <p>Uploads files</p>
            </a>
          </li>
          <li>
            <a href="./user">
                <i class="fa fa-user"></i>
                <p>User Profile</p>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div class="main-panel">
      <!-- Navbar -->
      <nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
        <div class="container-fluid">
          <div class="navbar-wrapper">
            <div class="navbar-toggle">
              <button type="button" class="navbar-toggler">
                <span class="navbar-toggler-bar bar1"></span>
                <span class="navbar-toggler-bar bar2"></span>
                <span class="navbar-toggler-bar bar3"></span>
              </button>
            </div>
            <a class="navbar-brand" href="#pablo">Diplo</a>
          </div>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-bar navbar-kebab"></span>
            <span class="navbar-toggler-bar navbar-kebab"></span>
            <span class="navbar-toggler-bar navbar-kebab"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end" id="navigation">
            <form>
              <div class="input-group no-border">
                <input type="text" value="" class="form-control" placeholder="Search...">
                <div class="input-group-append">
                  <div class="input-group-text">
                    <i class="fa fa-search"></i>
                  </div>
                </div>
              </div>
            </form>
            <ul class="navbar-nav">
              <li class="nav-item btn-rotate dropdown">
                <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <i class="fa fa-cog"></i>
                </a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
                  <a class="dropdown-item" href="#">Settings</a>
                  <a class="dropdown-item" href="#">Update Account</a>
                  <a class="dropdown-item" href="#">Logout</a>
                </div>
              </li>
              
            </ul>
          </div>
        </div>
      </nav>

      <div class="content" id="cardhead">
        <div class="row">
          <div class="col-md-12">
            <div class="card">
              <div class="card-header">
                <h4 class="card-title"><i class="fa fa-list-ul"></i> Questions list <a href="/create_question"><i class="fa fa-plus-circle"></i></a></h4>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <div class="table table-hover" style="padding: 10px;">                    
                  <% for(var i=0;i<questions.length;i++){ %>
                    <div class="row">
                      <div class="card" style="width: 100%; padding: 20px; border: 1px; border-style: solid;">
                            <div class="card-body">
                              <h5 class="card-title">Question <%=i+1%></h5>
                              <h7 class="card-subtitle mb-1 text-muted"><%=questions[i].topic %></h7>
                              <p class="card-text"><%=questions[i].body %></p>
                              <a class="card-link" data-toggle="collapse" href="#e<%=i+1%>"role="button" aria-expanded="false" ><i class="fa fa-angle-double-down"></i>Expand</a>
                              <a href="#" class="card-link"><i class="fa fa-pencil"></i>Edit</a>
                              <a href="#" class="card-link"><i class="fa fa-trashx"></i>Delete</a>
                              <a href="#" class="card-link"><i class="fa fa-copy"></i>Duplicate</a>
                              <a class="card-link" data-toggle="collapse" href="#imp<%=i+1%>"role="button" aria-expanded="false"><i class="fa fa-location-arrow" ></i>Import in</a>
                            </div>
                            <div class="card card-body collapse" id="e<%=i+1%>" >
                              <div class="container">
                                
                                <div class="row">
                                  <div class="col-md-2">Type</div>
                                  <div class="col-md-1">:</div>
                                  <div class="col"><%=questions[i].type %></div>
                                </div>
                                <div class="row">
                                  <div class="col-md-2">Topic</div>
                                  <div class="col-md-1">:</div>
                                  <div class="col"><%=questions[i].topic %></div>
                                </div>
                                <div class="row">
                                  <div class="col-md-2">Date Added</div>
                                  <div class="col-md-1">:</div>
                                  <div class="col"><%=questions[i].create_date %></div>
                                </div>
                                <div class="row">
                                  <div class="col-md-2">Last Modified</div>
                                  <div class="col-md-1">:</div>
                                  <div class="col"><%=questions[i].last_modified %></div>
                                </div> 
                                <div class="row">
                                    <div class="col-md-2">Answer</div>
                                    <div class="col-md-1">:</div>
                                    <div class="col"><%=questions[i].result %></div>
                                </div>
                              </div>
                            </div>
                            <div class="card card-body collapse" id="imp<%=i+1%>" >
                              <div class="container">
                                <div class="row">
                                  <div class="col-md-1">#</div>
                                  <div class="col">Test Name</div>
                                </div>
                                <%for(var j=0;j<questions[i].import_in.length;j++){%>
                                  <div class="row">
                                      <div class="col-md-1"><%=j+1%></div>
                                      <div class="col"><%=questions[i].import_in[j].name%></div>
                                  </div>  
                                <% }%>
                              </div>
                            </div>
                        </div>
                    </div>
                  <% } %>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <a class="btn" style="position: fixed; right:5px; bottom: 5px;" href="#cardhead"><i class="fa fa-chevron-circle-up" style="font-size: 30px;"></i></a>
        </div>



      <footer class="footer footer-black  footer-white ">
        <div class="container-fluid">
          <div class="row">
            <nav class="footer-nav">
              <ul>
                <li>
                  <a href="#" target="_blank">Diplo</a>
                </li>
                <li>
                  <a href="#" target="_blank">Blog</a>
                </li>
                <li>
                  <a href="#"   target="_blank">Licenses</a>
                </li>
              </ul>
            </nav>
          </div>
        </div>
      </footer>
      </div>
  </div>
  
  <!--   Core JS Files   -->
  <script src="assert/papper-dashboard/js/core/jquery.min.js"></script>
  <script src="assert/papper-dashboard/js/core/popper.min.js"></script>
  <script src="assert/papper-dashboard/js/core/bootstrap.min.js"></script>
  <script src="assert/papper-dashboard/js/plugins/perfect-scrollbar.jquery.min.js"></script>
  <!-- Chart JS -->
  <script src="assert/papper-dashboard/js/plugins/chartjs.min.js"></script>
  <!--  Notifications Plugin    -->
  <script src="assert/papper-dashboard/js/plugins/bootstrap-notify.js"></script>
  <!-- Control Center for Now Ui Dashboard: parallax effects, scripts for the example pages etc -->
  <script src="assert/papper-dashboard/js/paper-dashboard.min.js" type="text/javascript"></script>

</body>

</html>
